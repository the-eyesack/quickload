{
  "name": "quickload",
  "displayName": "Quickload",
  "version": "1.0.0",
  "description": "Open a loadout, or a set of links when a certain condition is met.",
  "author": "Isaac Wong",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "moment": "2.29.4",
    "plasmo": "0.82.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.3"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.0",
    "@types/chrome": "0.0.241",
    "@types/node": "20.4.2",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "postcss": "8.4.26",
    "prettier": "3.0.0",
    "typescript": "5.1.6"
  },
  "manifest": {
    "host_permissions": [
      "<all_urls>"
    ],
    "permissions": [
      "notifications",
      "tabs",
      "activeTab",
      "storage",
      "geolocation"
    ],
    "optional_permissions": [
      "geolocation"
    ]
  }
}
